{
  "name": "clack-tree-select",
  "version": "0.1.0",
  "description": "Beautiful, interactive tree selection prompts for Clack CLI apps",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "examples/tree-select.ts",
    "examples/integration-demo.ts",
    "examples/README.md",
    "examples/package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "unbuild",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "dev": "unbuild --watch",
    "lint": "biome lint --write",
    "format": "biome format --write"
  },
  "keywords": [
    "cli",
    "clack",
    "tree",
    "select",
    "prompt",
    "interactive",
    "terminal",
    "command-line",
    "file-picker",
    "directory-browser",
    "hierarchical",
    "multi-select"
  ],
  "author": {
    "name": "Titus Decali",
    "email": "titusdecali@gmail.com",
    "url": "https://github.com/titusdecali"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/titusdecali/clack-tree-select.git"
  },
  "bugs": {
    "url": "https://github.com/titusdecali/clack-tree-select/issues"
  },
  "homepage": "https://github.com/titusdecali/clack-tree-select#readme",
  "dependencies": {
    "@clack/core": "^0.5.0",
    "is-unicode-supported": "^1.3.0",
    "picocolors": "^1.0.0",
    "sisteransi": "^1.0.5"
  },
  "peerDependencies": {
    "@clack/prompts": ">=0.8.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.2",
    "@types/node": "^24.1.0",
    "typescript": "^5.8.3",
    "unbuild": "^3.6.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@9.14.2",
  "volta": {
    "node": "20.18.1"
  }
}